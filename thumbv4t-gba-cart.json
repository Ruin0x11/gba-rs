{
    "llvm-target": "thumbv4t-none-eabi-gcc",
    "data-layout": "e-p:32:32-m:e-i32:32-f32:32-n8:16:32-S8",
    "linker-flavor": "gcc",
    "linker": "arm-none-eabi-gcc",
    "pre-link-args": {
        "gcc": [
	    "-Tlinker.ld",
            "-nostartfiles",
            "-Wl,--print-map"
	]
    },
    "target-endian": "little",
    "target-pointer-width": "32",
    "target-c-int-width": "32",
    "arch": "arm",
    "os": "none",
    "features": "",
    "disable-redzone": true,
    "panic": "abort",
    "executables": true,
    "relocation_model": "static"
}
